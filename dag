digraph {
	"searches(t)"
	"sun(t)"
	"rain(t)"
	"searches(t-1)"
	"sun(t-1)"
	"rain(t-1)"
	"searches(t-1)" -> "searches(t)" [label=0.98]
	"rain(t-1)" -> "searches(t)" [label=0.66]
	"sun(t-1)" -> "sun(t)" [label=0.83]
	"rain(t-1)" -> "sun(t)" [label=0.41]
	"sun(t)" -> "rain(t)" [label=-0.11]
	"sun(t-1)" -> "rain(t)" [label=0.17]
	"rain(t-1)" -> "rain(t)" [label=0.61]
	{
		rank=same
	}
}
